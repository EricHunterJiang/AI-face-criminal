<Window x:Class="AIFaceDrawing.BugEnterWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AIFaceDrawing"
        mc:Ignorable="d"
        Title="BugEnterWin" Height="330" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="250"></RowDefinition>
            <RowDefinition Height=" 50"></RowDefinition>
        </Grid.RowDefinitions>
        <TextBox Grid.Row="0" Name="textBox" Width="300" Height="200" Text="{Binding BugText}"></TextBox>
        <Button Grid.Row="1" Width="100" Height="30" HorizontalAlignment="Left" Margin="100,0,0,0" Click="Yes_Click">Yes</Button>
    </Grid>
</Window>
